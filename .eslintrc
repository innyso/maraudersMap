{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "rules": {
    "react/jsx-filename-extension": 0,
    "no-use-before-define": 0,
    "global-require": 0,
    "react/sort-comp": 0,
    "no-underscore-dangle": 0,
    "max-len": 0,
    "object-curly-newline": 0
  }
}
